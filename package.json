{
  "name": "vue-start",
  "version": "0.2.0-0",
  "author": "xluobin@163.com",
  "description": "This is a mini project to study Vue and Webpack",
  "main": "./src/main.js",
  "license": "MIT",
  "scripts": {
    "lint:js": "eslint \"src/**/*.js\" --color",
    "lint:scss": "stylelint \"src/**/*.scss\" --color",
    "lint": "npm run lint:js && npm run lint:scss",
    "serve": "webpack-dev-server --color --progress --config ./build/webpack.dev.config",
    "serve:hmr": "webpack-dev-server --color --progress --hot --inline --config ./build/webpack.dev.config",
    "build": "webpack --color --progress --config ./build/webpack.prod.config",
    "build:stats": "webpack --color --progress --profile --json > stats.json --config ./build/webpack.prod.config && mv stats.json ./dist",
    "build:bundle-analyzer": "webpack-bundle-analyzer ./dist/stats.json -m static -O -r ./dist/bundle-analyzer.html",
    "release": "standard-version",
    "prerelease": "standard-version --prerelease"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.2",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-angular": "^7.5.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^3.0.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-include-assets-plugin": "^1.0.7",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.4.1",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "standard-version": "^8.0.1",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.5.4",
    "stylelint-webpack-plugin": "^0.10.5",
    "url-loader": "^2.0.0",
    "vue-loader": "^15.7.0",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.1.4",
    "element-ui": "^2.9.1",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "perfect-scrollbar": "^1.4.0",
    "socket.io-client": "^2.2.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.1.1",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.1",
    "vuex-class": "^0.3.2"
  }
}
