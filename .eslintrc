{
  "extends": "airbnb/base",
  "env": {
    "browser": true,
    "node": true,
    "jquery": true,
    "amd": true,
    "mocha": true,
    "jasmine": true,
    "es6": true,
    "commonjs": true
  },
  "rules": {
    "no-tabs": 0,
    "comma-dangle": 0,
    "linebreak-style": 0,
    "semi": [
      2,
      "never"
    ],
    "prefer-const": 0,
    "camelcase": 0,
    "no-console": 0,
    "space-before-function-paren": [
      0,
      "never"
    ],
    "spaced-comment": [
      1,
      "always"
    ],
    "indent": 0,
    "object-shorthand": 0,
    "func-names": 0,
    "consistent-return": 1,
    "max-len": [
      1,
      120,
      2
    ],
    "no-use-before-define": 1,
    "no-labels": 1,
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ],
    "no-loop-func": 1,
    "prefer-arrow-callback": 0,
    "global-require": 0,
    "no-new": 0,
    "no-else-return": 0,
    "import/no-unresolved": 0,
    "import/no-dynamic-require": 0,
    "import/no-extraneous-dependencies": 0
  }
}
